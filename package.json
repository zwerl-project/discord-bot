{
  "name": "@zwerl/discord-bot",
  "version": "1.0.0",
  "description": "Discord bot connected to the Zwerl Network.",
  "main": "src/main.ts",
  "author": "DownloadableFox <58178791+DownloadableFox@users.noreply.github.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "start": "node dist/main.js | pino-pretty",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "deploy": "yarn prisma:migrate && yarn start",
    "dev": "nodemon src/main.ts | pino-pretty"
  },
  "_moduleAliases": {
    "@utils": "./dist/utils/",
    "@config": "./dist/config/",
    "@events": "./dist/events/",
    "@commands": "./dist/commands/",
    "@services": "./dist/services/",
    "@interfaces": "./dist/interfaces/",
    "@middlewares": "./dist/middlewares/",
    "@tasks": "./dist/tasks/",
    "@web": "./dist/web/"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.14.0",
    "@prisma/client": "^4.10.0",
    "@types/glob": "^8.0.1",
    "@types/node-cron": "^3.0.7",
    "axios": "^1.3.0",
    "cors": "^2.8.5",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^8.1.0",
    "module-alias": "^2.2.2",
    "node-cron": "^3.0.2",
    "nodemon": "^2.0.20",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "prisma": "^4.10.0",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "ws": "^8.12.0",
    "ytdl-core": "^4.11.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.11.12",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "ts-node": "^10.9.1"
  }
}
